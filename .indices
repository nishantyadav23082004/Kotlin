fun main() {
    val alphabets = arrayOf("A","B","C","D","E")
    for(i in alphabets.indices){  // .indices stores the index of each element of array(alphabets) in i one by one
        println("Element at index "+i+" is "+alphabets[i])  /* i is storing the index
                                                               alphabets[i] is calling/getting the value at index i in the array(alphabets) */
    }
}
