fun main() {
    print("Enter a number: ")
    var num1:Int = readln().toInt()  // Taking string input from user and converting it to Integer
    print("Enter a number: ")
    var num2:Int = readln().toInt()  // Taking string input from user and converting it to Integer

    println(num1.compareTo(num2)>0) // Checking if num1>num2
    println(num1.compareTo(num2)<0)  // Checking if num1<num2
    println(num1.compareTo(num2)>=0)  // Checking if num1>=num2
    println(num1.compareTo(num2)<=0)  // Checking if num1<=num2


}
